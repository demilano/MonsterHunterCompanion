@model IEnumerable<MonsterHunterCompanion.Models.DamageCalculator.Weapons.Bow.Charge>

Charges

@for(int i=1 ; i < 5 ; i++)
{
    if (Model.Any(x => x.Level == i))
    {
        Html.RenderPartial("_chargeRow", Model.FirstOrDefault(x => x.Level == i));
    }
    else
    {
        Html.RenderPartial("_chargeRow", new MonsterHunterCompanion.Models.DamageCalculator.Weapons.Bow.Charge()
        {
            Level = i
        });
    }
}